# .gitignore for Jekyll

# Node modules
node_modules/

# Logs and databases
*.log
*.sqlite3
*.env

# Gem specific files
/.bundle/
/vendor/bundle/
/vendor/cache/
/vendor/gems/
/vendor/ruby/

# Jekyll specific files
_site/
.sass-cache/
.jekyll-cache/
.jekyll-metadata
.jekyll-cache/

# Temporary files
*.tmp
*.temp
*.swp

# Files generated by the package manager
.yarn-integrity
package-lock.json
yarn.lock

# Byebug command history
.byebug_history

# Other
.DS_Store
